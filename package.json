{
  "name": "secure-gha",
  "version": "1.0.0",
  "description": "This provide ability to run prerequisites before running anything else",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -o dist/secure-gha src/secure-gha.ts && ncc build -o dist/waitfor src/waitfor.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajatjindal/secure-gha.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "spin"
  ],
  "author": "Rajat Jindal",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "@octokit/rest": "^19.0.7",
    "@types/fs-extra": "^11.0.1",
    "@types/uuid": "^9.0.1",
    "fs-extra": "^11.1.0",
    "toml": "^3.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@vercel/ncc": "^0.36.1",
    "prettier": "2.8.4",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "mkdirp": "^3.0.1",
    "ts-loader": "^9.4.1"
  }
}